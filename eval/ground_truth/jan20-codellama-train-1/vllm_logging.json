{
  "version": 1,
  "disable_existing_loggers": false,
  "handlers": {
    "file": {
      "class": "logging.FileHandler",
      "filename": "/mnt/exp/jan20-codellama-train-1/vllm.log",
      "level": "INFO",
      "formatter": "simple"
    }
  },
  "formatters": {
    "simple": {
      "format": "%(asctime)s %(levelname)s %(name)s: %(message)s"
    }
  },
  "loggers": {
    "vllm": {
      "handlers": ["file"],
      "level": "INFO",
      "propagate": false
    }
  }
}
